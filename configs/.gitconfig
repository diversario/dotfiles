[grep]
	lineNumber = true
	extendedRegexp = true
[core]
	excludesfile = /Users/diversario/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
